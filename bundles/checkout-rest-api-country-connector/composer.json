{
  "name": "fond-of-kudu/checkout-rest-api-country-connector",
  "description": "provide the country selection for the endpoint checkout-data",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "p-fischer@gmx.net"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fond-of-kudu/checkout-rest-api-country-extension": "^1.0.0",
    "spryker/checkout-rest-api-extension": "^1.5.0",
    "spryker/country": "^3.1.0 || ^4.0.0",
    "spryker/glossary-storage": "^1.0.0",
    "spryker/locale": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfKudu\\": "src/FondOfKudu/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfKudu\\": "tests/FondOfKudu/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
