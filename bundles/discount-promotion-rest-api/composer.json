{
  "name": "fond-of-kudu/discount-promotion-rest-api",
  "description": "",
  "license": "MIT",
  "authors": [
    {
      "name": "Pascal Fischer",
      "email": "p-fischer@gmx.net"
    }
  ],
  "require": {
    "php": ">=8.0",
    "spryker/discount-promotions-rest-api": "^1.4.0",
    "spryker/price-product-storage": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "spryker/product-resource-alias-storage": "^1.0.0",
    "spryker/product-storage": "^1.13.0",
    "spryker/currency": "^3.2.0 || ^4.0.0",
    "spryker/discount": "^9.34.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "*",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfKudu\\": "src/FondOfKudu/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfKudu\\": "tests/FondOfKudu/",
      "Generated\\": "src/Generated/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
